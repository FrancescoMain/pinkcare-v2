{
  "version": 2,
  "name": "pinkcare-api",
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node",
      "config": {
        "bundler": "nft",
        "externalNodeModules": [
          "pg"
        ],
        "maxDuration": 30
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
